<body>
<!--start-body-->
			<!--star-wrapper-demo-->
      <div class="NAV">
        <nav>
          <ul>
            <li><a href="/logOut">LogOut</a></li>
            <li><a href="/profile">Profile</a></li>
            <li><a href="/">Home</a></li>
          </ul>

        </nav>

      </div>
			<div class="wrapper-demo">
				<h1>Friend List</h1>
				<!--start-wrapper-dropdown-2-->
					<div id="dd" class="wrapper-dropdown-2" tabindex="1">Friends<i><img src="images/friendsimages/menu.png"/></i>
									 <!-- start search-->
				    <div class="search-box">
					    <div id="sb-search" class="sb-search">
							<form>
								<input class="sb-search-input" placeholder="Enter your search term..." type="search" name="search" id="search">
								<input class="sb-search-submit" type="submit" value="">
								<span class="sb-icon-search"> </span>
							</form>
						</div>
				    </div>
				    <!-- search-scripts -->
					<script src="js/friendsjs/classie.js"></script>
					<script src="js/friendsjs/uisearch.js"></script>
						<script>
							new UISearch( document.getElementById( 'sb-search' ) );
						</script>
					<!-- /search-scripts -->
 				<!-- end search-->
          <% for(var i=0; i<friends.length; i++) {%>
							<ul class="dropdown">
								<div class="friends-grids">
									<div class="grids-left">
										<img src="images/friendsimages/a1.png" />
									</div>
									<div class="friend-grid"><p class="flowers">14k</p></div>

									<div class="grids-right">
										<h2><%=friends[i].local.username%></h2>
                    <h2><a href="mailto:<%=friends[i].local.email%>">Go to <%=friends[i].local.username%> email </a></h2>
										<!--<img src="images/heart.png" />-->
									<!-- <ul class="grids-right-info">
											<li class="user">107</li>
											<li class="camera">23</li>
											<li class="chat">6</li>
										<div class="clear"> </div>
									</ul> -->
									</div>

									<div class="clear"> </div>
								</div>
                  <% } %>

							</ul>
					</div>
			</div>
			<!--end-wrapper-dropdown-2-->
			<!--start-script-->
			<script type="text/javascript">
							function DropDown(el) {
								this.dd = el;
								this.initEvents();
							}
							DropDown.prototype = {
								initEvents : function() {
									var obj = this;

									obj.dd.on('click', function(event){
										$(this).toggleClass('active');
										event.stopPropagation();
									});
								}
							}
							$(function() {

								var dd = new DropDown( $('#dd') );

								$(document).click(function() {
									// all dropdowns
									$('.wrapper-dropdown-2').removeClass('active');
								});

							});
			</script>
			<!--end-script-->
			<!--end-wrapper-demo-->
